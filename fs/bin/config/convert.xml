<config>
	<dicts>
	<!-- 默认字典转换是双向的，如果是单向的话，需要特别注明 way="1":从fix到sgit(正向)；way="-1":从sgit到fix(反向)-->
		<dict number='8008' name='testSide'>
			<item fix="A" sgit="0" way="1"/>
			<item fix="B" sgit="1" way="-1"/>
		</dict>	
		<dict number='77' name='OpenClose'>
			<item fix="O" sgit="0" description='OPEN'/>
			<item fix="C" sgit="1" description='CLOSE'/>
			<!-- 还有平今平昨？-->
		</dict>		
		<dict number='40' name='OrdType'>
			<item fix="1" sgit="1"  description='MARKET'/>
			<item fix="2" sgit="2" description='LIMIT'/>
		</dict>
		<dict number='54' name='Side'>
			<item fix="1" sgit="0"  description='BUY'/>
			<item fix="2" sgit="1" description='SELL'/>
		</dict>
	</dicts>
	
	<month desc="一位月份和两位月份的相互转化">
		<item in="01" out="F" />
		<item in="02" out="G" />
		<item in="03" out="H" />
		<item in="04" out="J" />
		<item in="05" out="K" />
		<item in="06" out="M" />
		<item in="07" out="N" />
		<item in="08" out="Q" />
		<item in="09" out="U" />
		<item in="10" out="V" />
		<item in="11" out="X" />
		<item in="12" out="Z" />
	</month>
	
	<symbols desc="type 0:交易所原始代码 1:彭博代码，*:年份 ?:月份">
		<!-- 中金所品种 -->
		<symbol name="IF" desc="股指期货">
			<item type="0" format="IF**??" re="IF\d{4}" yearpos="2" yearlen="2" monthpos="4" monthlen="2" desc="正则表达式匹配IF后4个数字，年月开始位置(从0开始)，年月长度"/>
			<item type="1" format="IFB?* Index" re="IFB[A-Z]{1}\d{1} Index" yearpos="4" yearlen="1" monthpos="3" monthlen="1" desc="正则表达式匹配IFB后1个英文字符、1个数字"/>
		</symbol>
		
		<!-- 上期所品种-->
		<symbol name="au" desc="沪金">
			<item type="0" format="au**??" re="au\d{4}" yearpos="2" yearlen="2" monthpos="4" monthlen="2"/>
			<item type="1" format="AUA?* Comdty" re="AUA[A-Z]{1}\d{1} Comdty" yearpos="4" yearlen="1" monthpos="3" monthlen="1"/>
		</symbol>
		
		<!-- 大商所品种-->
		<symbol name="m" desc="豆粕">
			<item type="0" format="m**??" re="m\d{4}" yearpos="2" yearlen="2" monthpos="4" monthlen="2"/>
			<item type="1" format="AE?* Comdty" re="AE[A-Z]{1}\d{1} Comdty" yearpos="3" yearlen="1" monthpos="2" monthlen="1"/>
		</symbol>
		
		<!-- 郑商所品种-->
		<symbol name="FG" desc="玻璃">
			<item type="0" format="FG*??" re="FG\d{3}" yearpos="2" yearlen="1" monthpos="3" monthlen="2"/>
			<item type="1" format="FGL?* Comdty" re="FGL[A-Z]{1}\d{1} Comdty" yearpos="4" yearlen="1" monthpos="3" monthlen="1"/>
		</symbol>
		<symbol name="WH" desc="强麦">
			<item type="0" format="WH*??" re="WH\d{3}" yearpos="2" yearlen="1" monthpos="3" monthlen="2"/>
			<item type="1" format="VN?* Comdty" re="VN[A-Z]{1}\d{1} Comdty" yearpos="3" yearlen="1" monthpos="2" monthlen="1"/>
		</symbol>
	</symbols>
</config>

